{
  "name": "vite-starter",
  "description": "ViteJS starter template for vanilla JavaScript with some basic dependencies.",
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "vanilla",
    "javascript",
    "boilerplate",
    "boilerplate-template",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template"
  ],
  "scripts": {
    "dev": "vite --host --open",
    "format": "prettier --write \"./**/*.{js,jsx,json,css}\"",
    "build": "vite build && vite build --config vite.config.min.js --emptyOutDir=false"
  },
  "devDependencies": {
    "@spiriit/vite-plugin-svg-spritemap": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.5.1",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.4",
    "svgo": "^3.3.2",
    "vite": "6.0.7",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
